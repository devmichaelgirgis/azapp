<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/activity_register_rlRegisterHeaderSocial"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/snot"
                android:paddingTop="@dimen/_5sdp"
                android:visibility="visible">

                <LinearLayout
                    android:id="@+id/reg_header_second"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingEnd="@dimen/_16sdp">

                    <android.support.v7.widget.AppCompatImageView
                        android:id="@+id/register_ivBack"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:paddingEnd="@dimen/_16sdp"
                        android:paddingStart="@dimen/_16sdp"
                        android:rotation="180"
                        android:tint="@color/white"
                        app:srcCompat="@drawable/ic_blue_back" />

                    <com.exceedgulf.alainzoo.views.CustomTextview
                        android:id="@+id/register_tvHeader"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:text="@string/register"
                        android:textAlignment="viewStart"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_25ssp"
                        app:fontFamily="@string/cross_town" />

                    <com.exceedgulf.alainzoo.views.CustomButton
                        android:id="@+id/register_btnSkip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@null"
                        android:text="@string/skip"
                        android:textAlignment="viewEnd"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_15ssp"
                        android:visibility="gone"
                        app:fontFamily="@string/avenir_next_medium" />


                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_below="@id/reg_header_second"
                    android:orientation="vertical">

                    <android.support.v7.widget.AppCompatImageView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_60sdp"
                        android:layout_alignParentStart="true"
                        android:scaleType="fitXY"
                        app:srcCompat="@drawable/register_one_auto_mirrored" />


                    <com.exceedgulf.alainzoo.views.CustomButton
                        android:id="@+id/register_tvSignIn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:background="@null"
                        android:text="@string/sign_in_fc"
                        android:textAlignment="viewEnd"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_15ssp"
                        android:visibility="gone"
                        app:fontFamily="@string/avenir_next_medium" />


                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/activity_register_rlRegisterHeader"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/yellow_orange"
                android:paddingTop="@dimen/_5sdp"
                android:visibility="gone">

                <LinearLayout
                    android:id="@+id/act_signin_llheader"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingEnd="@dimen/_16sdp">

                    <android.support.v7.widget.AppCompatImageView
                        android:id="@+id/register_social_ivBack"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:paddingEnd="@dimen/_16sdp"
                        android:paddingStart="@dimen/_16sdp"
                        android:rotation="180"
                        android:tint="@color/white"
                        app:srcCompat="@drawable/ic_blue_back" />

                    <com.exceedgulf.alainzoo.views.CustomTextview
                        android:id="@+id/signin_tvHeader"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="start"
                        android:text="@string/register"
                        android:textAlignment="viewStart"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_25ssp"
                        app:fontFamily="@string/cross_town" />

                    <com.exceedgulf.alainzoo.views.CustomButton
                        android:id="@+id/signin_btnregister"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@null"
                        android:text="@string/sign_in_fc"
                        android:textAlignment="viewEnd"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_15ssp"
                        android:visibility="gone"
                        app:fontFamily="@string/avenir_next_medium" />
                </LinearLayout>

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_60sdp"
                    android:layout_below="@id/act_signin_llheader"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/signin_header_auto_mirrored" />

            </RelativeLayout>


        </LinearLayout>


        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:scrollbars="none"
            android:visibility="visible">

            <LinearLayout
                android:id="@+id/root_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="vertical"
                android:paddingTop="@dimen/_10sdp">

                <LinearLayout
                    android:id="@+id/activity_register_llRegister"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_16sdp"
                            android:layout_marginStart="@dimen/_16sdp"
                            android:orientation="vertical">

                            <com.exceedgulf.alainzoo.views.CustomTextview
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/tell_us_more_about_yourself"
                                android:textAlignment="viewStart"
                                android:textAllCaps="false"
                                android:textColor="@color/slate_grey"
                                android:textSize="@dimen/_16ssp"
                                app:fontFamily="@string/avenir_next_medium" />


                            <com.exceedgulf.alainzoo.views.CustomTextview
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/more_about_you_helps_us_customize_your_experience"
                                android:textColor="@color/cool_grey_two"
                                android:textSize="@dimen/_11ssp"
                                app:fontFamily="@string/avenir_next_regular" />

                        </LinearLayout>
                    </FrameLayout>


                    <com.exceedgulf.alainzoo.views.RoundedImageView
                        android:id="@+id/register_ivAvatar"
                        android:layout_width="@dimen/_60sdp"
                        android:layout_height="@dimen/_60sdp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="@dimen/_15sdp"
                        app:civ_border="true"
                        app:civ_border_color="@color/white_badge_text"
                        app:civ_border_width="@dimen/_2sdp"
                        app:srcCompat="@mipmap/ic_launcher_round" />

                    <com.exceedgulf.alainzoo.views.CustomTextview
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:tag="@string/star"
                        android:text="@string/your_name"
                        android:textAllCaps="false"
                        android:textColor="@color/cool_grey_two"
                        android:textSize="@dimen/_11ssp"
                        app:fontFamily="@string/avenir_next_regular" />

                    <com.exceedgulf.alainzoo.views.CustomEdittext
                        android:id="@+id/register_edtName"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:background="@drawable/bg_edittext"
                        android:inputType="textPersonName"
                        android:maxLines="1"
                        android:padding="@dimen/_5sdp"
                        android:textAlignment="viewStart"
                        android:textColor="@color/black_71"
                        android:textDirection="anyRtl"
                        app:fontFamily="@string/avenir_next_regular" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <com.exceedgulf.alainzoo.views.CustomTextview
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_16sdp"
                                android:layout_marginStart="@dimen/_16sdp"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:text="@string/date_of_birth"
                                android:textAllCaps="false"
                                android:textColor="@color/cool_grey_two"
                                android:textSize="@dimen/_11sdp"
                                app:fontFamily="@string/avenir_next_regular" />

                            <com.exceedgulf.alainzoo.views.CustomTextview
                                android:id="@+id/register_tvDOB"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_35sdp"
                                android:layout_marginEnd="@dimen/_16sdp"
                                android:layout_marginStart="@dimen/_16sdp"
                                android:layout_marginTop="@dimen/_8sdp"
                                android:background="@drawable/bg_edittext"
                                android:gravity="center_vertical|start"
                                android:maxLines="1"
                                android:padding="@dimen/_5sdp"
                                android:textAlignment="viewStart"
                                android:textColor="@color/cool_grey_two"
                                android:textSize="@dimen/_15ssp"
                                app:fontFamily="@string/avenir_next_regular" />

                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <com.exceedgulf.alainzoo.views.CustomTextview
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_16sdp"
                                android:layout_marginStart="@dimen/_16sdp"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:tag="@string/star"
                                android:text="@string/gender"
                                android:textAllCaps="false"
                                android:textColor="@color/cool_grey_two"
                                android:textSize="@dimen/_11sdp"
                                app:fontFamily="@string/avenir_next_regular" />

                            <Spinner
                                android:id="@+id/register_spnGender"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_35sdp"
                                android:layout_marginEnd="@dimen/_16sdp"
                                android:layout_marginStart="@dimen/_16sdp"
                                android:layout_marginTop="@dimen/_8sdp"
                                android:background="@drawable/bg_edittext"
                                android:padding="@dimen/_5sdp"
                                android:textAlignment="viewStart"
                                android:textColor="@color/cool_grey_two" />

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <com.exceedgulf.alainzoo.views.CustomTextview
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_16sdp"
                                android:layout_marginStart="@dimen/_16sdp"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:text="@string/nationality"
                                android:textAllCaps="false"
                                android:textColor="@color/cool_grey_two"
                                android:textSize="@dimen/_11sdp"
                                app:fontFamily="@string/avenir_next_regular" />


                            <Spinner
                                android:id="@+id/register_spnNationality"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_35sdp"
                                android:layout_marginEnd="@dimen/_16sdp"
                                android:layout_marginStart="@dimen/_16sdp"
                                android:layout_marginTop="@dimen/_8sdp"
                                android:background="@drawable/bg_edittext"
                                android:padding="@dimen/_5sdp"
                                android:textAlignment="viewStart"
                                android:textColor="@color/cool_grey_two" />

                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <com.exceedgulf.alainzoo.views.CustomTextview
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_16sdp"
                                android:layout_marginStart="@dimen/_16sdp"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:text="@string/emirate"
                                android:textAllCaps="false"
                                android:textColor="@color/cool_grey_two"
                                android:textSize="@dimen/_11sdp"
                                app:fontFamily="@string/avenir_next_regular" />

                            <Spinner
                                android:id="@+id/register_spnEmirate"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_35sdp"
                                android:layout_marginEnd="@dimen/_16sdp"
                                android:layout_marginStart="@dimen/_16sdp"
                                android:layout_marginTop="@dimen/_8sdp"
                                android:background="@drawable/bg_edittext"
                                android:padding="@dimen/_5sdp"
                                android:textAlignment="viewStart"
                                android:textColor="@color/cool_grey_two" />

                        </LinearLayout>

                    </LinearLayout>

                    <com.exceedgulf.alainzoo.views.CustomButton
                        android:id="@+id/register_btnRegister"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_gravity="end"
                        android:layout_marginBottom="@dimen/_30sdp"
                        android:layout_marginEnd="@dimen/_32sdp"
                        android:layout_marginStart="@dimen/_32sdp"
                        android:layout_marginTop="@dimen/_30sdp"
                        android:background="@drawable/rounded_corner_btn"
                        android:text="@string/save_and_register"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_15ssp"
                        app:fontFamily="@string/avenir_next_medium" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/activity_register_llSocial"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_16sdp"
                            android:layout_marginStart="@dimen/_16sdp"
                            android:orientation="vertical">

                            <com.exceedgulf.alainzoo.views.CustomTextview
                                android:id="@+id/language_tvTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/register_with_us"
                                android:textAlignment="viewStart"
                                android:textAllCaps="false"
                                android:textColor="@color/slate_grey"
                                android:textSize="@dimen/_16ssp"
                                app:fontFamily="@string/avenir_next_medium" />

                            <com.exceedgulf.alainzoo.views.CustomTextview
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:text="@string/registeration_opens_up_a_whole_jungle_of_features"
                                android:textAlignment="viewStart"
                                android:textAllCaps="false"
                                android:textColor="@color/slate_grey_two"
                                android:textSize="@dimen/_11ssp"
                                app:fontFamily="@string/avenir_next_regular" />


                            <com.exceedgulf.alainzoo.views.CustomTextview
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_20sdp"
                                android:visibility="gone"
                                android:text="@string/register_via_social_media"
                                android:textAlignment="viewStart"
                                android:textAllCaps="false"
                                android:textColor="@color/cool_grey_two"
                                android:textSize="@dimen/_11ssp"
                                app:fontFamily="@string/avenir_next_medium" />

                            <RelativeLayout
                                android:id="@+id/activity_register_rlFacebook"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_35sdp"
                                android:layout_marginTop="@dimen/_8sdp"
                                android:background="@drawable/ic_facebook_btn_bg">


                                <android.support.v7.widget.AppCompatImageView
                                    android:id="@+id/ivfacebook"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_centerVertical="true"
                                    app:srcCompat="@drawable/ic_facebook" />

                                <com.exceedgulf.alainzoo.views.CustomTextview
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_toEndOf="@id/ivfacebook"
                                    android:gravity="start"
                                    android:text="@string/login_using_facebook"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/_13ssp"
                                    app:fontFamily="@string/avenir_next_medium   " />


                            </RelativeLayout>


                        </LinearLayout>

                    </FrameLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:paddingEnd="@dimen/_16sdp"
                        android:paddingStart="@dimen/_16sdp">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_centerVertical="true"
                            android:layout_toStartOf="@id/tvor"
                            android:background="@color/cool_grey" />

                        <com.exceedgulf.alainzoo.views.CustomTextview
                            android:id="@+id/tvor"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:paddingEnd="@dimen/_5sdp"
                            android:paddingStart="@dimen/_5sdp"
                            android:text="@string/or"
                            android:textAllCaps="true"
                            android:textColor="@color/cool_grey"
                            android:textSize="@dimen/_12ssp"
                            app:fontFamily="@string/avenir_next_bold" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_centerVertical="true"
                            android:layout_toEndOf="@id/tvor"
                            android:background="@color/cool_grey" />
                    </RelativeLayout>

                    <com.exceedgulf.alainzoo.views.CustomTextview
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:tag="@string/star"
                        android:text="@string/email_address"
                        android:textAllCaps="false"
                        android:textColor="@color/cool_grey_two"
                        android:textSize="@dimen/_11sdp"
                        app:fontFamily="@string/avenir_next_regular" />

                    <com.exceedgulf.alainzoo.views.CustomEdittext
                        android:id="@+id/signin_edtemail"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:background="@drawable/bg_edittext"
                        android:inputType="textEmailAddress"
                        android:maxLines="1"
                        android:padding="@dimen/_5sdp"
                        android:textAlignment="viewStart"
                        android:textColor="@color/black_71"
                        android:textDirection="anyRtl"
                        app:fontFamily="@string/avenir_next_regular" />

                    <com.exceedgulf.alainzoo.views.CustomTextview
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:tag="@string/star"
                        android:text="@string/phone_number"
                        android:textAllCaps="false"
                        android:textColor="@color/cool_grey_two"
                        android:textSize="@dimen/_11sdp"
                        app:fontFamily="@string/avenir_next_regular" />

                    <com.exceedgulf.alainzoo.views.CustomEdittext
                        android:id="@+id/register_edtmobile"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:background="@drawable/bg_edittext"
                        android:inputType="phone"
                        android:hint="@string/hint_register_mobile_no"
                        android:maxLines="1"
                        android:padding="@dimen/_5sdp"
                        android:textAlignment="viewStart"
                        android:textColor="@color/black_71"
                        android:textDirection="anyRtl"
                        app:fontFamily="@string/avenir_next_regular" />

                    <com.exceedgulf.alainzoo.views.CustomTextview
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:tag="@string/star"
                        android:text="@string/password"
                        android:textAllCaps="false"
                        android:textColor="@color/cool_grey_two"
                        android:textSize="@dimen/_11sdp"
                        app:fontFamily="@string/avenir_next_regular" />

                    <com.exceedgulf.alainzoo.views.CustomEdittext
                        android:id="@+id/register_edtpassword"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:background="@drawable/bg_edittext"
                        android:inputType="textPassword"
                        android:maxLines="1"
                        android:padding="@dimen/_5sdp"
                        android:textAlignment="viewStart"
                        android:textColor="@color/black_71"
                        android:textDirection="anyRtl"
                        app:fontFamily="@string/avenir_next_regular" />

                    <com.exceedgulf.alainzoo.views.CustomTextview
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:tag="@string/star"
                        android:text="@string/confirm_password"
                        android:textAllCaps="false"
                        android:textColor="@color/cool_grey_two"
                        android:textSize="@dimen/_11sdp"
                        app:fontFamily="@string/avenir_next_regular" />

                    <com.exceedgulf.alainzoo.views.CustomEdittext
                        android:id="@+id/register_edtConfirmpassword"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginEnd="@dimen/_16sdp"
                        android:layout_marginStart="@dimen/_16sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:background="@drawable/bg_edittext"
                        android:inputType="textPassword"
                        android:maxLines="1"
                        android:padding="@dimen/_5sdp"
                        android:textAlignment="viewStart"
                        android:textColor="@color/black_71"
                        android:textDirection="anyRtl"
                        app:fontFamily="@string/avenir_next_regular" />

                    <com.exceedgulf.alainzoo.views.CustomButton
                        android:id="@+id/activity_register_btnCreateAccount"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_gravity="end"
                        android:layout_marginBottom="@dimen/_30sdp"
                        android:layout_marginEnd="@dimen/_32sdp"
                        android:layout_marginStart="@dimen/_32sdp"
                        android:layout_marginTop="@dimen/_30sdp"
                        android:background="@drawable/rounded_corner_btn"
